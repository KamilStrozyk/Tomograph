<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object id="main_window" class="tk.Toplevel">
    <property name="height">720</property>
    <property name="width">1280</property>
    <child>
      <object id="main_notebook" class="ttk.Notebook">
        <property name="height">700</property>
        <property name="width">700</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object id="main_tab" class="ttk.Notebook.Tab">
            <property name="text" translatable="yes">Main
</property>
            <child>
              <object id="main_frame" class="ttk.Frame">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
                <child>
                  <object id="add_file_button" class="ttk.Button">
                    <property name="command">addFile</property>
                    <property name="state">normal</property>
                    <property name="takefocus">false</property>
                    <property name="text" translatable="yes">Add File</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="ipadx">42</property>
                      <property name="propagate">True</property>
                      <property name="row">6</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="run_button" class="ttk.Button">
                    <property name="command">run</property>
                    <property name="text" translatable="yes">Run</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="columnspan">2</property>
                      <property name="ipadx">42</property>
                      <property name="propagate">True</property>
                      <property name="row">7</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="alpha_spinbox" class="ttk.Spinbox">
                    <property name="cursor">arrow</property>
                    <property name="font">TkDefaultFont</property>
                    <property name="from_">0</property>
                    <property name="increment">0.5</property>
                    <property name="justify">left</property>
                    <property name="state">normal</property>
                    <property name="takefocus">false</property>
                    <property name="to">360</property>
                    <property name="validate">none</property>
                    <property name="wrap">false</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">False</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="alpha_label" class="ttk.Label">
                    <property name="anchor">w</property>
                    <property name="font">TkTextFont</property>
                    <property name="text" translatable="yes">Alpha Value</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="emmiters_spinbox" class="ttk.Spinbox">
                    <property name="font">TkDefaultFont</property>
                    <property name="from_">0</property>
                    <property name="increment">1</property>
                    <property name="justify">left</property>
                    <property name="state">normal</property>
                    <property name="to">10000</property>
                    <property name="wrap">false</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">False</property>
                      <property name="row">3</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="emmiters_label" class="ttk.Label">
                    <property name="anchor">w</property>
                    <property name="font">TkTextFont</property>
                    <property name="text" translatable="yes">Emmiters count</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="image_canvas" class="tk.Canvas">
                    <property name="background">black</property>
                    <property name="height">250</property>
                    <property name="relief">flat</property>
                    <property name="width">250</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="pady">100</property>
                      <property name="propagate">True</property>
                      <property name="row">10</property>
                      <property name="rowspan">10</property>
                      <property name="sticky">n</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="sinogram_canvas" class="tk.Canvas">
                    <property name="background">black</property>
                    <property name="cursor">arrow</property>
                    <property name="height">300</property>
                    <property name="relief">flat</property>
                    <property name="width">300</property>
                    <layout>
                      <property name="column">18</property>
                      <property name="padx">60</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                      <property name="rowspan">10</property>
                      <property name="sticky">n</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="recon_canvas" class="tk.Canvas">
                    <property name="background">black</property>
                    <property name="height">300</property>
                    <property name="relief">flat</property>
                    <property name="width">300</property>
                    <layout>
                      <property name="column">18</property>
                      <property name="pady">200</property>
                      <property name="propagate">True</property>
                      <property name="row">9</property>
                      <property name="rowspan">10</property>
                      <property name="sticky">s</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="Scale_1" class="ttk.Scale">
                    <property name="command">slider</property>
                    <property name="from_">0</property>
                    <property name="length">300</property>
                    <property name="orient">horizontal</property>
                    <property name="to">100</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="columnspan">2</property>
                      <property name="propagate">True</property>
                      <property name="row">9</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="progress_label" class="ttk.Label">
                    <property name="anchor">w</property>
                    <property name="font">TkTextFont</property>
                    <property name="text" translatable="yes">Progress
</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">9</property>
                      <property name="sticky">nw</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="span_spinbox" class="ttk.Spinbox">
                    <property name="cursor">arrow</property>
                    <property name="font">TkTextFont</property>
                    <property name="from_">0</property>
                    <property name="increment">1</property>
                    <property name="justify">left</property>
                    <property name="state">normal</property>
                    <property name="to">10000</property>
                    <property name="wrap">false</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">False</property>
                      <property name="row">5</property>
                      <property name="sticky">sw</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="span_label" class="ttk.Label">
                    <property name="anchor">w</property>
                    <property name="font">TkTextFont</property>
                    <property name="text" translatable="yes">Span</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">4</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object id="dicom_tab" class="ttk.Notebook.Tab">
            <property name="state">normal</property>
            <property name="text" translatable="yes">Dicom
</property>
            <child>
              <object id="dicom_frame" class="ttk.Frame">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
                <child>
                  <object id="id_label" class="ttk.Label">
                    <property name="compound">top</property>
                    <property name="text" translatable="yes">Id</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="id_entry" class="ttk.Entry">
                    <property name="exportselection">true</property>
                    <property name="justify">left</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="name_label" class="ttk.Label">
                    <property name="text" translatable="yes">Name</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="name_entry" class="ttk.Entry">
                    <property name="font">TkDefaultFont</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="surname_label" class="ttk.Label">
                    <property name="text" translatable="yes">Surname</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">4</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="surname_entry" class="ttk.Entry">
                    <property name="cursor">arrow</property>
                    <property name="show">•</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object id="Label_7" class="ttk.Label">
                    <property name="text" translatable="yes">Label_7</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">6</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
